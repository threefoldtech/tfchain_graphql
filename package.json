    "dotenv": "^16.0.0",
    "substrate-tfgrid-ts-types": "^0.4.0",
    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@subsquid/cli": "^0.2.0",
    "@subsquid/substrate-metadata-explorer": "^0.0.7",
    "@subsquid/substrate-typegen": "^0.4.0",
    "typescript": "^4.6.2"
  }
