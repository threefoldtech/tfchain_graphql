{
  "name": "graphql_tfgrid",
  "private": "true",
  "version": "2.8.0-rc7",
  "description": "GraphQL server and Substrate indexer. Generated with â™¥ by Hydra-CLI",
  "author": "",
  "license": "ISC",
  "scripts": {
    "update": "npx npm-check-updates --filter /subsquid/ --upgrade && npm i -f",
    "build": "rm -rf lib && tsc",
    "db:create-migration": "sqd db create-migration",
    "db:init": "node init-countries.js",
    "db:up": "docker-compose up -d db",
    "process": "node db/init.js && node init-countries.js && node -r dotenv/config lib/processor.js",
    "api": "squid-graphql-server",
    "codegen": "sqd codegen",
    "typegen": "squid-substrate-typegen typegen/typegen.json",
    "lint": "npm run lint:prod -- --fix",
    "lint:prod": "eslint . --ext .ts --ignore-path .gitignore --max-warnings 0"
  },
  "dependencies": {
    "@subsquid/archive-registry": "1.0.18",
    "@subsquid/cli": "2.0.1",
    "@subsquid/graphql-server": "3.3.0",
    "@subsquid/ss58": "^0.1.2",
    "@subsquid/substrate-processor": "2.2.0",
    "@subsquid/typeorm-migration": "0.1.4",
    "@subsquid/typeorm-store": "0.2.0",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "lodash": "^4.17.21",
    "pg": "^8.7.3",
    "pg-format": "^1.0.4",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@subsquid/substrate-metadata-explorer": "1.0.9",
    "@subsquid/substrate-typegen": "2.0.0",
    "@subsquid/typeorm-codegen": "0.3.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "16.11.47",
    "typescript": "4.7.4"
  }
}